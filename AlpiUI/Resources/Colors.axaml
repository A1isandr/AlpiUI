    <SolidColorBrush x:Key="AlpiInfoBrush100" Color="{DynamicResource AlpiInfoColor100}"/>
    <SolidColorBrush x:Key="AlpiInfoBrush40" Color="{DynamicResource AlpiInfoColor40}"/>
    <SolidColorBrush x:Key="AlpiInfoBrush0" Color="{DynamicResource AlpiInfoColor0}"/>
    
    <SolidColorBrush x:Key="AlpiTextBrush" Color="{DynamicResource AlpiTextColor}"/>
    <SolidColorBrush x:Key="AlpiDisabledTextBrush" Color="{DynamicResource AlpiDisabledTextColor}"/>
</ResourceDictionary>
