    <SolidColorBrush x:Key="AlpiBackgroundBrush40" Color="{DynamicResource AlpiBackgroundColor40}"/>
    <SolidColorBrush x:Key="AlpiBackgroundBrush30" Color="{DynamicResource AlpiBackgroundColor30}"/>
    <SolidColorBrush x:Key="AlpiBackgroundBrush20" Color="{DynamicResource AlpiBackgroundColor20}"/>
    <SolidColorBrush x:Key="AlpiBackgroundBrush10" Color="{DynamicResource AlpiBackgroundColor10}"/>
    <SolidColorBrush x:Key="AlpiBackgroundBrush0" Color="{DynamicResource AlpiBackgroundColor0}"/>
    
    <SolidColorBrush x:Key="AlpiDangerBrush100" Color="{DynamicResource AlpiDangerColor100}"/>
    <SolidColorBrush x:Key="AlpiDangerBrush40" Color="{DynamicResource AlpiDangerColor40}"/>
    <SolidColorBrush x:Key="AlpiDangerBrush0" Color="{DynamicResource AlpiDangerColor0}"/>
    
    <SolidColorBrush x:Key="AlpiWarningBrush100" Color="{DynamicResource AlpiWarningColor100}"/>
    <SolidColorBrush x:Key="AlpiWarningBrush40" Color="{DynamicResource AlpiWarningColor40}"/>
